{
	"redirects": [],
	"rewrites": [
		{
			"source": "/mailmerge/:match*",
			"destination": "https://mailmerge.vercel.app/:match*"
		},
		{
			"source": "/ponder/:match*",
			"destination": "https://ponder.vercel.app/:match*"
		},
		{ "source": "/(.*)", "destination": "/index.html" }
	],
	"github": {
		"autoAlias": true,
		"enabled": true,
		"silent": true
	},
	"cleanUrls": true,
	"trailingSlash": false,
	"public": false
}
